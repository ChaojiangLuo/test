{
  "name": "Event Tracking with Google Analytics",
  "version": "2.0.0",
  "description": "A sample extension which uses Google Analytics to track usage.",
  "app": {
    "launch": {
      "local_path": "popup.html",
      "container": "shell",
      "width": 300,
      "height": 300
    }
  },
  "platform_app": true,
  "icons": {
    "48": "analytics-extension-icon-48.png",
    "128": "analytics-extension-icon-128.png"
  },

  "permission": [
    "chrome://resources"
  ],

  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "content_security_policy": "script-src 'self' chrome://resources; object-src 'self'"
}
